<div class="navbar navbar-default navbar-static-top" role="navigation">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="/"><%= menu.top.title %> <img class="icon" src="/img/nnov.png"></a>
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
                <% menu.top.left.items.forEach(function(item) { %>
                    <% if(item.sub_items != null) { %>
                        <li class="dropdown">
                            <% if(item.icon != null) { %>
                                <a href="<%=item.url %>" class="dropdown-toggle" data-toggle="dropdown"><i class="fa <%=item.icon %> fa-fw"></i> <%=item.text %> <b class="caret"></b></a>
                            <% } else { %>
                                <a href="<%=item.url %>" class="dropdown-toggle" data-toggle="dropdown"><%=item.text %> <b class="caret"></b></a>
                            <% } %>
                            <ul class="dropdown-menu">
                                <% item.sub_items.forEach(function(sub_item) { %>
                                    <% if (sub_item.icon != null) { %>
                                        <li><a href="<%=sub_item.url %>"><i class="fa <%=sub_item.icon %> fa-fw"></i><%= sub_item.text %></a></li>
                                    <% } else { %>
                                        <li><a href="<%=sub_item.url %>"><%= sub_item.text %></a></li>
                                    <% } %>
                                <% }) %>
                            </ul>
                        </li>
                    <% } else { %>
                        <% if(item.icon != null) { %>
                            <li><a href="<%= item.url %>"><i class="fa <%=item.icon %> fa-fw"></i> <%=item.text %></a></li>
                        <% } else { %>
                            <li><a href="<%= item.url %>"><%=item.text %></a></li>
                        <% } %>
                    <% } %>
                <% }) %>
            </ul>
            <ul class="nav navbar-nav navbar-right">
                <% menu.top.right.items.forEach(function(item) { %>
                    <% if(item.icon != null) { %>
                        <li><a href="<%= item.url %>"><i class="fa <%=item.icon%> fa-fw"></i> <%=item.text %></a></li>
                    <% } else { %>
                        <li><a href="<%= item.url %>"><%=item.text %></a></li>
                    <% } %>
                <% }) %>
            </ul>
        </div><!--/.nav-collapse -->
    </div>
</div>


